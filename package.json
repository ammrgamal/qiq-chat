{
  "name": "qiq-chat",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "algoliasearch": "^4.24.0",
    "express": "^4.19.2",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.4.0",
    "xlsx": "^0.18.5",
    "joi": "^17.12.0",
    "sqlite3": "^5.1.7",
    "mssql": "^9.3.0",
    "openai": "^4.57.0",
    "cli-progress": "^3.12.0",
    "axios": "^1.7.7"
  },
  "scripts": {
    "start": "node server.js",
    "apply:algolia-settings": "ts-node src/search/applySettings.ts",
    "test": "npm run test:search",
  "algolia:diagnose": "node scripts/algolia-diagnose.mjs",
  "algolia:configure": "node enrichment-engine/src/index.js --configure-index || node -e \"(async()=>{const s=(await import('./enrichment-engine/src/algoliaService.js')).default; await s.configureIndex();})()\"",
  "test:search": "jest __tests__/algoliaMapper.spec.ts __tests__/emailValidation.spec.ts -u",
    "enrich:run": "node rules-engine/scripts/run-enrichment.mjs",
    "enrich:kaspersky12": "node rules-engine/scripts/enrich-products12-kaspersky.mjs",
    "report:enrichment": "node rules-engine/scripts/report-enrichment.mjs --brand=Kaspersky --list=12",
    "algolia:sync:kaspersky12": "node rules-engine/scripts/algolia-sync-products12.mjs --brand=Kaspersky --list=12",
    "email:report:kaspersky12": "node scripts/email-enrichment-report.mjs --brand=Kaspersky --list=12",
    "algolia:browse:nested": "node scripts/algolia-browse-nested.mjs"
    "db:schema:extend": "node rules-engine/scripts/extend-products-schema.mjs"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^24.5.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2"
  }
}
